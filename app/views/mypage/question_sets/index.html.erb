<div>
  新しくボートマッチを作成する
  <%= form_for([:mypage, @question_set]) do |f| %>
    タイトル：<%= f.text_field :title %>
    <%= f.submit "Create Question Set" %>
  <% end %>
</div>
<div>
  作成したボートマッチ一覧
  <ul>
    <% @question_sets.each do |question_set| %>
      <li><%= link_to question_set.title, mypage_question_set_path(question_set) %></li>
    <% end %>
  </ul>
</div>
