<h2>診断結果</h2>
<div class="yourresult-content">
  <% @your_results.each do |result| %>
    <div class="each-party-yourresult clearfix">
      <div class="party-name"><%= result[:party].name  %></div>
      <div class="party-point-wrap">
        <div class="point-bar j__party_<%= result[:party].id %>">
          <div class="point-num"><%= number_to_percentage(result[:percent], precision: 0) %></div>
        </div>
      </div>
    </div>
  <% end %>
</div>
<div class="object-votematch">
  診断ボートマッチ：<%= link_to @question_set.title, question_set_path(@question_set) %>（<%= @question_set.user_id %>）
</div>
<div class="questions-container">
<h3>あなたの回答</h3>
  <% @question_set.questions.each do |question| %>
    <div class="question-content clearfix feature_<%= question.id %>" id="<%= question.id %>">
      <div class="question-title-show">
        <div class="question-title-style">
          <%= question.title %>
        </div>
      </div>
      <%= @question_set.selected_choice_tags('agree', @try_history.choices[question.id.to_s]) %>
      <%= @question_set.selected_choice_tags('neutral', @try_history.choices[question.id.to_s]) %>
      <%= @question_set.selected_choice_tags('opposition', @try_history.choices[question.id.to_s]) %>
    </div>
  <% end %>
</div>
